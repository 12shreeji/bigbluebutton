		/>
		
		<module name="ViewersModule" url="${project.artifactId}-${project.version}-ViewersModule.swf" 
			uri="rtmp://192.168.0.215/bigbluebutton" 
			host="http://192.168.0.215/bigbluebutton/api/enter"
			allowKickUser="false"
		/>
			
